<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Cadastrar Evento</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ServletCadastroEventos</display-name>
    <servlet-name>ServletCadastroEventos</servlet-name>
    <servlet-class>br.lisianthus.visao.ServletCadastroEventos</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ServletAluno</display-name>
    <servlet-name>ServletAluno</servlet-name>
    <servlet-class>br.lisianthus.visao.ServletAluno</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletCadastroEventos</servlet-name>
    <url-pattern>/eventos</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletAluno</servlet-name>
    <url-pattern>/aluno</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ServletSessionLogin</display-name>
    <servlet-name>ServletSessionLogin</servlet-name>
    <servlet-class>br.lisianthus.visao.ServletSessionLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletSessionLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>  
</web-app>